@model DocenteViewModel
<h1>Modifica l'Email del Docente</h1>

<form method="post">



<div class="form-group">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" readonly/>
    <span asp-validation-for="Nome" class="Text-danger"></span>
</div>

<div class="form-group">
    <label asp-for="Cognome"></label>
    <input asp-for="Cognome" class="form-control" readonly/>
    <span asp-validation-for="Cognome" class="Text-danger"></span>
</div>


<div class="form-group">
    <label asp-for="Email"></label>
    <input asp-for="Email" class="form-control" readonly/>
    <span asp-validation-for="Email" class="Text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Telefono"></label>
    <input asp-for="Telefono" class="form-control" />
    <span asp-validation-for="Telefono" class="Text-danger"></span>
</div>

<input type="submit" value="Salva Modifica" class="btn btn-primary" />
<input type="reset" value="Reset" class="btn btn-primary" />
<a class="btn btn-primary" asp-controller="Corsi" asp-action="Index">Indietro</a>

</form>